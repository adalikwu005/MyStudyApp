<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mystudyapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_app_test"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_app_test_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".QuizActivity" />
        <activity android:name=".SearchPastquestion" />
        <activity android:name=".StartExams" />
        <activity android:name=".CgpaCalculator" />
        <activity android:name=".AppDashboard" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>